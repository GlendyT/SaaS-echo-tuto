{
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.tsc.autoDetect": "off",
  "search.exclude": {
    "**/node_modules": true,
    "**/.turbo": true,
    "**/convex/_generated": true,
    "**/.next": true,
    "**/dist": true,
    "**/build": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.turbo/**": true,
    "**/convex/_generated/**": true,
    "**/.next/**": true
  }
}